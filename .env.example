# Primary LLM Provider (Google Gemini - FREE)
GEMINI_API_KEY=your-gemini-api-key-here

# Backup LLM Providers (Optional)
OPENAI_API_KEY=sk-your-openai-key-here

# LLM Configuration
GEMINI_MODEL=gemini-1.5-flash
OPENAI_MODEL=gpt-4o
TEMPERATURE=0.3

# Fallback Configuration
USE_FALLBACK=true

# API Configuration
API_HOST=localhost
API_PORT=8000

# Optional: Set to 'openai' to force OpenAI, 'gemini' to force Gemini
# FORCE_PROVIDER=
